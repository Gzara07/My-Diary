<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity5"
    android:background="@color/background">

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/_______"
        android:layout_marginTop="30dp"/>

    <LinearLayout
        android:id="@+id/liner1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintTop_toBottomOf="@id/liner3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="170dp">

        <Button
            android:id="@+id/button3"
            android:layout_width="76dp"
            android:layout_height="89dp"
            android:layout_marginEnd="20dp"
            android:text="Прочее"
            android:textSize="12sp"
            app:layout_constraintEnd_toStartOf="@+id/button6"
            android:background="@color/green_w"/>

        <Button
            android:id="@+id/button6"
            android:layout_width="76dp"
            android:layout_height="89dp"
            android:layout_marginEnd="20dp"
            android:text="Задачи"
            android:textSize="12sp"
            app:layout_constraintEnd_toStartOf="@+id/button7"
            android:background="@color/green_b"/>

        <Button
            android:id="@+id/button7"
            android:layout_width="79dp"
            android:layout_height="89dp"
            android:layout_marginEnd="20dp"
            android:text="Календарь"
            android:textSize="12sp"
            app:layout_constraintEnd_toStartOf="@+id/button9"
            android:background="@color/green_w"/>

        <Button
            android:id="@+id/button9"
            android:layout_width="76dp"
            android:layout_height="89dp"
            android:text="Мое"
            android:textSize="12sp"
            android:background="@color/green_w"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/liner2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/imageView11"
        android:gravity="left"
        android:layout_marginTop="40dp">

        <TextView
            android:id="@+id/textView20"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="15.10"
            android:textSize="18sp"
            android:textColor="@color/black"
            app:layout_constraintBottom_toTopOf="@+id/imageView9"
            app:layout_constraintEnd_toEndOf="@+id/imageView9"
            app:layout_constraintStart_toStartOf="@+id/imageView9"/>

        <ImageView
            android:id="@+id/imageView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/cjplfnm"/>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/liner3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:gravity="left"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/liner2"
        android:layout_marginTop="30dp">

        <TextView
            android:id="@+id/textView21"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="21.10"
            android:textSize="18sp"
            android:textColor="@color/black"
            app:layout_constraintBottom_toTopOf="@+id/imageView10"
            app:layout_constraintEnd_toEndOf="@+id/imageView10"
            app:layout_constraintStart_toStartOf="@+id/imageView10"/>

        <ImageView
            android:id="@+id/imageView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/_________1_" />

    </LinearLayout>



</androidx.constraintlayout.widget.ConstraintLayout>